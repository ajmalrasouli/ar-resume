{
  "version": 2,
  "builds": [
    { "src": "api/ai/*.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/ai/(.*)", "dest": "/api/ai/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
